package oops;
import java.util.Scanner;
public class finding_copysign_by_math {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
		System.out.println("the  number is "+Math.copySign(a,b));
		

	}

}
