package oops;
import java.util.Scanner;
public class floor_function_by_math {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		double a=sc.nextDouble();
		int b=sc.nextInt();
		System.out.println("the floor number is "+Math.floor(a));
		System.out.println("the ceil number is "+Math.ceil(a));

	}

}
