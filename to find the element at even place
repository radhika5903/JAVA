package Trial_Project;
import java.util.Scanner;
public class to_find_array_ele_at_even_place {

	public to_find_array_ele_at_even_place() {
		// TODO Auto-generated constructor stub
		Scanner sc=new Scanner(System.in);
		System.out.println("enter the value");
		int val=sc.nextInt();
		int []a=new int[val];
		for(int i=0;i<=val;i++) {
			a[i]=sc.nextInt();
		}
		for(int j=0;j<=val;j++) {
			if(j%2==0) {
				System.out.println(a[j]);
			}
			
		}
	}

}
